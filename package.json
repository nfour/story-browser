{
  "name": "story-browser",
  "version": "2.3.2",
  "description": "A React component story browser as React components.",
  "module": "x/mjs",
  "bin": {
    "makeStoryMap": "./x/cjs/bin/makeStoryMap.js"
  },
  "main": "./x/cjs/index.js",
  "types": "./x/cjs/index.d.ts",
  "files": [
    "x/*"
  ],
  "scripts": {
    "build": "pnpm build:clean; pnpm build:cjs; pnpm build:mjs",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:mjs": "tsc -p tsconfig.mjs.json",
    "build:clean": "rm -rf x/*",
    "test": "node ./x/cjs/bin/makeStoryMap '**/*.stories.tsx' --output testProject/nesting/folder/STORY_MAP.ts",
    "start": "vite",
    "dev": "pnpm start"
  },
  "dependencies": {
    "@componentdriven/csf": "^0.0.2-alpha.0",
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@mui/icons-material": "^5.0.3",
    "@mui/lab": "^5.0.0-alpha.50",
    "@mui/material": "^5.0.3",
    "@types/lodash": "^4.14.172",
    "antd": "^4.16.13",
    "change-case": "^4.1.2",
    "fast-glob": "^3.2.5",
    "lodash": "^4.17.21",
    "material-icons": "^1.2.1",
    "react-resize-detector": "^6.7.1",
    "react-stroller": "^1.8.0",
    "react-virtualized": "^9.22.3",
    "react-virtualized-tree": "^3.4.1",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/yargs": "^16.0.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "eslint-config-nfour": "^1.3.0",
    "eslint-plugin-only-warn": "1.0.3",
    "history": "^5.0.0",
    "mobx": "^6.3.3",
    "mobx-react-lite": "^3.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.4.3",
    "vite": "^2.6.7",
    "xroute": "^6.1.0"
