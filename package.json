{
  "name": "story-browser",
  "version": "5.5.1",
  "description": "A React component story browser as React components.",
  "bin": {
    "makeStoryMap": "./x/cjs/bin/makeStoryMap.js"
  },
  "main": "./x/cjs/index.js",
  "module": "./x/mjs/index.js",
  "types": "./x/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./x/mjs/index.js",
      "require": "./x/cjs/index.js"
    }
  },
  "files": [
    "x/*"
  ],
  "scripts": {
    "build": "pnpm build:clean; pnpm build:cjs; pnpm build:mjs;",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:mjs": "tsc -p tsconfig.mjs.json",
    "build:clean": "rimraf ./x/*",
    "build:declarations": "tsc -p tsconfig.declarations.json",
    "build:bin": "tsc -p tsconfig.bin.json",
    "build:vite": "vite build",
    "test": "pnpm storyMap",
    "storyMap": "node ./x/cjs/bin/makeStoryMap '**/*.stories.tsx' --output testProject/nesting/folder/STORY_MAP.ts",
    "dev:dist": "vite dev",
    "dev": "vite dev -c vite.testProject.config.ts"
  },
  "peerDependencies": {
    "@emotion/css": ">= 11",
    "@emotion/react": ">= 11",
    "@emotion/styled": ">= 11",
    "react": ">= 17",
    "react-dom": ">= 17"
  },
  "dependencies": {
    "@componentdriven/csf": "^0.0.2-alpha.0",
    "@types/lodash": "^4.14.172",
    "change-case": "^4.1.2",
    "fast-glob": "^3.2.5",
    "framer-motion": "6",
    "lodash": "^4.17.21",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@emotion/css": "^11.10.0",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@babel/core": "^7.18.6",
    "@types/node": "^14.14.37",
    "@types/react": "^18.0.19",
    "@types/react-dom": "^18.0.6",
    "@types/yargs": "^16.0.1",
    "@vitejs/plugin-react": "^2.0.0",
    "eslint-config-nfour": "^3.2.0",
    "history": "^5.0.0",
    "mobx": "^6.3.3",
    "mobx-react-lite": "^3.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "typescript": "~4.8.2",
    "vite": "^3.0.2",
    "xroute": "^6.8.0"
  },
  "resolutions": {
    "workerpool": "6.1.0"
  }
}
