{
  "name": "story-browser",
  "version": "0.1.1-0",
  "description": "A React component story browser as React components.",
  "module": "x/mjs",
  "bin": "./x/cjs/bin",
  "main": "./x/cjs/index.js",
  "types": "./x/cjs/index.d.ts",
  "files": [ "x/*" ],
  "eslintConfig": {
    "ignorePatterns": "x",
    "extends": "eslint-config-nfour/.eslintrc.react"
  },
  "scripts": {
    "build": "yarn build:clean; yarn build:cjs; yarn build:mjs",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:mjs": "tsc -p tsconfig.mjs.json",
    "build:clean": "rm -rf x/*",
    "start": "snowpack dev",
    "npmRelease": "yarn build && cd ./x && npm publish"
  },
  "peerDependencies": {},
  "dependencies": {
    "@componentdriven/csf": "^0.0.2-alpha.0",
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "fast-glob": "^3.2.5",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@snowpack/plugin-babel": "^2.1.6",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/snowpack-env": "^2.3.3",
    "@types/yargs": "^16.0.1",
    "eslint-config-nfour": "^1.3.0",
    "history": "^5.0.0",
    "mobx": "^6.1.8",
    "mobx-react-lite": "^3.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "snowpack": "^3.0.13",
    "typescript": "^4.2.3",
    "xroute": "^3.1.0"
  },
  "resolutions": {
    "workerpool": "6.1.0"
  }
}
